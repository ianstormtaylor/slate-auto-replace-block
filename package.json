{
  "name": "slate-auto-replace-block",
  "description": "A Slate plugin to automatically set properties on a block when the user types a string of matching text.",
  "version": "0.2.2",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/slate-auto-replace-block.git",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist ./node_modules",
    "dist": "babel ./lib --out-dir ./dist",
    "example": "browserify ./example/index.js --debug --transform babelify > ./example/build.js",
    "gh-pages": "npm run dist && npm run example && gh-pages --dist ./example",
    "lint": "eslint 'lib/**/*.js' 'example/index.js'",
    "prepublish": "npm run dist",
    "start": "http-server ./example -p 8888",
    "test": "npm run lint",
    "watch": "npm-run-all --parallel --print-label watch:dist watch:example start",
    "watch:dist": "babel ./lib --out-dir ./dist --watch",
    "watch:example": "watchify ./example/index.js --debug --transform babelify --outfile ./example/build.js"
  },
  "dependencies": {
    "escape-regex-string": "^1.0.4",
    "type-of": "^2.0.1"
  },
  "peerDependencies": {
    "slate": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^3.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-react": "^5.2.2",
    "gh-pages": "^0.11.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.3.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "slate": "^0.8.0",
    "watchify": "^3.7.0"
  },
  "keywords": [
    "slate"
  ]
}
